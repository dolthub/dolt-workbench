{
  "private": true,
  "name": "dolt-workbench",
  "description": "SQL workbench for MySQL and PostgreSQL databases",
  "homepage": "https://github.com/dolthub/dolt-workbench",
  "version": "0.3.23",
  "main": "app/background.js",
  "author": "DoltHub",
  "packageManager": "yarn@4.5.0",
  "scripts": {
    "build:graphql": "yarn --cwd ../graphql-server build",
    "download:dolt": "node build/downloadDolt.js",
    "dev:app": "yarn build:graphql && nextron --renderer-port 7777",
    "build:dmg": "yarn build:graphql && nextron build --config ./build/builder-dmg-config.yaml",
    "build:mas": "yarn build:graphql && nextron build --config ./build/builder-mas-config.yaml",
    "build:win": "yarn build:graphql && nextron build --win --x64 --config ./build/builder-win-config.yaml",
    "build:linux": "yarn build:graphql && nextron build --linux --arm64 --config ./build/builder-linux-config.yaml",
    "postinstall": "electron-builder install-app-deps",
    "sign-dmg": "cd build/mac && ./sign_and_notarize.sh \"../../dist/DoltWorkbench-mac-arm64.dmg\"",
    "prettier": "prettier --check 'renderer/{components,contexts,hooks,lib,pages}/**/*.{js,ts,jsx,tsx,css}' 'main/*.{js,ts,jsx,tsx}' 'main/**/*.{js,ts,jsx,tsx}'",
    "prettier-fix": "prettier --write 'renderer/{components,contexts,hooks,lib,pages}/**/*.{js,ts,jsx,tsx,css}' 'main/*.{js,ts,jsx,tsx}' 'main/**/*.{js,ts,jsx,tsx}'",
    "lint": "yarn lint-js-errors && yarn lint-css",
    "lint-js-errors": "eslint --quiet --cache --ext .ts,.js,.tsx,.jsx ./renderer",
    "lint-js": "eslint --cache --ext .ts,.js,.tsx,.jsx ./renderer",
    "lint-css": "stylelint \"renderer/**/*.css\"",
    "lint-fix": "eslint --quiet --fix --ext .ts,.js,.tsx,.jsx ./renderer",
    "generate-types": "graphql-codegen --config ./renderer/codegen.yml",
    "test": "jest --env=jest-environment-jsdom ./renderer",
    "dev": "next dev ./renderer -p 3002",
    "build": "next build ./renderer",
    "compile": "tsc -b",
    "find:unused": "npx unimported",
    "yalc": "yalc link",
    "yalc:clean": "yalc remove --all",
    "ci": "yarn prettier && yarn compile && yarn lint && yarn test && yarn build",
    "clean": "tsc -b --clean && yarn yalc:clean"
  },
  "dependencies": {
    "@apollo/client": "^3.9.9",
    "@dolthub/react-components": "^0.2.9",
    "@dolthub/react-contexts": "^0.1.0",
    "@dolthub/react-hooks": "^0.1.8",
    "@dolthub/web-utils": "^0.1.9",
    "@react-icons/all-files": "^4.1.0",
    "ace-builds": "^1.37.5",
    "apollo-upload-client": "^17.0.0",
    "chance": "^1.1.11",
    "classnames": "^2.3.2",
    "commit-graph": "^2.3.12",
    "diff": "^5.1.0",
    "electron-serve": "^1.3.0",
    "electron-store": "^10.0.1",
    "github-markdown-css": "^5.3.0",
    "graphql": "^16.8.1",
    "isomorphic-unfetch": "^4.0.2",
    "localforage": "^1.10.0",
    "localforage-getitems": "^1.4.2",
    "lodash": "^4.17.21",
    "next": "^15.2.4",
    "next-useragent": "^2.8.0",
    "node-sql-parser": "^4.17.0",
    "react": "^19.0.0",
    "react-ace": "^14.0.1",
    "react-copy-to-clipboard": "^5.1.0",
    "react-data-grid": "7.0.0-beta.44",
    "react-diff-viewer": "^3.1.1",
    "react-dom": "^19.0.0",
    "react-infinite-scroller": "^1.2.6",
    "react-timeago": "^8.0.0",
    "reactflow": "^11.11.4",
    "sharp": "^0.33.4",
    "swr": "^2.3.3",
    "tailwindcss": "^3.4.15",
    "url-parse": "^1.5.10",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@electron/osx-sign": "^2.0.0",
    "@graphql-codegen/cli": "^5.0.5",
    "@graphql-codegen/fragment-matcher": "^5.1.0",
    "@graphql-codegen/typescript-operations": "^4.6.1",
    "@graphql-codegen/typescript-react-apollo": "^4.0.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/adm-zip": "^0",
    "@types/apollo-upload-client": "^17",
    "@types/chance": "^1.1.5",
    "@types/diff": "^5.2.1",
    "@types/jest": "^29.5.6",
    "@types/lodash": "^4.14.202",
    "@types/node": "^22.5.2",
    "@types/react": "^19.0.10",
    "@types/react-copy-to-clipboard": "^5.0.6",
    "@types/react-dom": "^19.0.4",
    "@types/react-infinite-scroller": "^1.2.3",
    "@types/react-timeago": "^4.1.4",
    "@types/url-parse": "^1",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^8.29.0",
    "adm-zip": "^0.5.16",
    "autoprefixer": "^10.4.19",
    "babel-jest": "^29.7.0",
    "cssnano": "^7.0.7",
    "electron": "^36.3.2",
    "electron-builder": "^26.0.12",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-next": "^15.1.3",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-plugin-css-modules": "^2.12.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^28.3.0",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-testing-library": "^6.2.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "next": "^15.2.4",
    "nextron": "^9.5.0",
    "npm-run-all2": "^7.0.2",
    "postcss": "^8.4.49",
    "postcss-preset-env": "^10.1.3",
    "prettier": "^3.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-select-event": "^5.5.1",
    "rimraf": "^6.0.1",
    "stylelint": "^16.15.0",
    "stylelint-config-recommended": "^14.0.0",
    "tar": "^7.4.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.7.3",
    "webpack-preprocessor-loader": "^1.3.0",
    "yalc": "^1.0.0-pre.53"
  },
  "resolutions": {
    "@babel/runtime": "7.26.10",
    "@types/react": "18.3.5",
    "webpack": "5.94.0"
  }
}
