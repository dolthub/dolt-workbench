    "localforage": "^1.10.0",
    "localforage-getitems": "^1.4.2",
    "lodash": "^4.17.21",
    "next": "^14.2.10",
    "next-useragent": "^2.8.0",
    "node-sql-parser": "^4.17.0",
    "react": "^18.3.1",
    "react-ace": "^12.0.0",
    "react-copy-to-clipboard": "^5.1.0",
