{
  "name": "@dolt-sql-workbench/web",
  "version": "0.1.0",
  "author": "DoltHub",
  "private": true,
  "packageManager": "yarn@4.5.0",
  "scripts": {
    "ci": "yarn prettier && yarn compile && yarn lint && yarn test && yarn build",
    "clean": "tsc -b --clean && yarn yalc:clean",
    "compile": "tsc -b",
    "dev": "next dev -p 3002",
    "build": "next build",
    "start": "next start",
    "lint": "yarn lint-js-errors && yarn lint-css",
    "lint-js-errors": "eslint --quiet --cache --ext .ts,.js,.tsx,.jsx .",
    "lint-js": "eslint --cache --ext .ts,.js,.tsx,.jsx .",
    "lint-css": "stylelint \"**/*.css\"",
    "lint-fix": "eslint --quiet --fix --ext .ts,.js,.tsx,.jsx .",
    "generate-types": "graphql-codegen --config codegen.yml",
    "prettier": "prettier --check '{components,contexts,hooks,lib,pages}/**/*.{js,ts,jsx,tsx,css}'",
    "prettier-fix": "prettier --write '{components,contexts,hooks,lib,pages}/**/*.{js,ts,jsx,tsx,css}'",
    "test": "jest --env=jest-environment-jsdom",
    "find:unused": "npx unimported",
    "yalc": "yalc link",
    "yalc:clean": "yalc remove --all"
  },
  "dependencies": {
