{
  "private": true,
  "name": "dolt-workbench",
  "description": "SQL workbench for MySQL and PostgreSQL databases",
  "version": "0.3.0",
  "main": "app/background.js",
  "author": "DoltHub",
  "packageManager": "yarn@4.5.0",
  "scripts": {
    "dev": "nextron",
    "build": "nextron build",
    "postinstall": "electron-builder install-app-deps",
    "clean": "npm-run-all 'clean:*'",
    "clean:misc": "rimraf node_modules '*/node_modules' '*/.eslintcache' '*/*.tsbuildinfo' '*/dist' renderer/.next",
    "compile": "npm-run-all 'compile:*'",
    "compile:renderer": "tsc -b renderer",
    "ci:renderer": " yarn prettier renderer && yarn compile:renderer&& yarn lint && yarn test && yarn build",
    "clean:renderer": "tsc -b --clean renderer && yarn yalc:clean",
    "yalc": "cd renderer && yalc link",
    "yalc:clean": "yalc remove --all"
  },
  "build": {
    "appId": "com.dolthub.dolt-workbench",
    "productName": "Dolt Workbench",
    "buildVersion": "0.3.0",
    "copyright": "Copyright Â© 2024 <DoltHub Inc>",
    "files": [
      "app",
      "package.json",
      "!node_modules/**/*",
      "node_modules/ajv",
      "node_modules/ajv-formats",
      "node_modules/atomically",
      "node_modules/conf",
      "node_modules/debounce-fn",
      "node_modules/dot-prop",
      "node_modules/electron-store",
      "node_modules/electron-serve",
      "node_modules/env-paths",
      "node_modules/fast-deep-equal",
      "node_modules/fast-uri",
      "node_modules/is-obj",
      "node_modules/json-schema-traverse",
      "node_modules/json-schema-typed",
      "node_modules/locate-path",
      "node_modules/mimic-fn",
      "node_modules/onetime",
      "node_modules/p-locate",
      "node_modules/p-try",
      "node_modules/pkg-up",
      "node_modules/require-from-string",
      "node_modules/semver",
      "node_modules/type-fest"
    ],
    "extraFiles": [
      {
        "from": "../graphql-server/dist",
        "to": "graphql-server/dist"
      },
      {
        "from": "../graphql-server/node_modules",
        "to": "graphql-server/node_modules"
      },
      {
        "from": "build/mac/Assets.car",
        "to": "Resources/Assets.car"
      },
      {
        "from": "build/mac/AppIcon.icns",
        "to": "Resources/AppIcon.icns"
      }
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "icon": "build/mac/AppIcon.icns",
      "extendInfo": {
        "CFBundleIconName": "AppIcon",
        "CFBundleIconFile": "AppIcon"
      },
      "target": [
        {
          "target": "dmg"
        },
        {
          "target": "mas",
          "arch": "universal"
        }
      ],
      "artifactName": "${productName}-mac-${arch}.${ext}",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "asarUnpack": [
        "**/*.node"
      ],
      "provisioningProfile": "build/mac/AppleDevelopment.provisionprofile",
      "entitlements": "build/mac/entitlements.plist",
      "entitlementsInherit": "build/mac/entitlements.inherit.plist"
    },
    "mas": {
      "category": "public.app-category.developer-tools",
      "icon": "build/mac/AppIcon.icns",
      "extendInfo": {
        "CFBundleIconName": "AppIcon",
        "CFBundleIconFile": "AppIcon",
        "CFBundleDisplayName": "Dolt Workbench"
      },
      "artifactName": "${productName}-mac-${arch}.${ext}",
      "type": "distribution",
      "notarize": {
        "teamId": "FK9YU86WZ5"
      },
      "x64ArchFiles": "*",
      "hardenedRuntime": false,
      "gatekeeperAssess": false,
      "provisioningProfile": "build/mac/MacAppStore.provisionprofile",
      "entitlements": "build/mac/entitlements.mas.plist",
      "entitlementsInherit": "build/mac/entitlements.mas.inherit.plist"
    }
  },
  "dependencies": {
    "@apollo/client": "^3.9.9",
    "@dolthub/react-components": "^0.2.6",
    "@dolthub/react-contexts": "^0.1.0",
    "@dolthub/react-hooks": "^0.1.7",
    "@dolthub/web-utils": "^0.1.8",
    "@react-icons/all-files": "^4.1.0",
    "ace-builds": "^1.34.2",
    "apollo-upload-client": "^17.0.0",
    "chance": "^1.1.11",
    "classnames": "^2.3.2",
    "commit-graph": "^2.1.1",
    "diff": "^5.1.0",
    "electron-serve": "^1.3.0",
    "electron-store": "^8.2.0",
    "github-markdown-css": "^5.3.0",
    "graphql": "^16.8.1",
    "isomorphic-unfetch": "^4.0.2",
    "localforage": "^1.10.0",
    "localforage-getitems": "^1.4.2",
    "lodash": "^4.17.21",
    "next": "^14.2.10",
    "next-useragent": "^2.8.0",
    "node-sql-parser": "^4.17.0",
    "react": "^18.3.1",
    "react-ace": "^12.0.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-data-grid": "7.0.0-beta.44",
    "react-diff-viewer": "^3.1.1",
    "react-dom": "^18.2.0",
    "react-infinite-scroller": "^1.2.6",
    "react-timeago": "^7.2.0",
    "reactflow": "^11.11.4",
    "sharp": "^0.33.4",
    "swr": "^2.2.4",
    "tailwindcss": "^3.4.13"
  },
  "devDependencies": {
    "@electron/osx-sign": "^1.3.1",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/fragment-matcher": "^5.0.0",
    "@graphql-codegen/typescript-operations": "^4.2.3",
    "@graphql-codegen/typescript-react-apollo": "^4.0.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/apollo-upload-client": "^17",
    "@types/chance": "^1.1.5",
    "@types/diff": "^5.2.1",
    "@types/jest": "^29.5.6",
    "@types/lodash": "^4.14.202",
    "@types/node": "^22.5.2",
    "@types/react": "^18.3.5",
    "@types/react-copy-to-clipboard": "^5.0.6",
    "@types/react-dom": "^18.2.18",
    "@types/react-infinite-scroller": "^1.2.3",
    "@types/react-timeago": "^4.1.4",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.14.1",
    "autoprefixer": "^10.4.19",
    "babel-jest": "^29.7.0",
    "cssnano": "^6.0.3",
    "electron": "^31.0.1",
    "electron-builder": "^25.1.7",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-next": "^14.2.7",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-plugin-css-modules": "^2.12.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^28.3.0",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-testing-library": "^6.2.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "next": "^14.2.4",
    "nextron": "^9.1.0",
    "npm-run-all": "4.1.5",
    "postcss": "^8.4.40",
    "postcss-preset-env": "^9.4.0",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-select-event": "^5.5.1",
    "rimraf": "^6.0.1",
    "stylelint": "^16.6.1",
    "stylelint-config-recommended": "^14.0.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.4.5",
    "webpack-preprocessor-loader": "^1.3.0",
    "yalc": "^1.0.0-pre.53"
  },
  "resolutions": {
    "@types/react": "18.3.5"
  }
}