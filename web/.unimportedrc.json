{
  "entry": ["renderer/pages/**/*.{js,ts,tsx}", "main/*.{js,ts,jsx,tsx}"],
  "ignorePatterns": [
    "**/node_modules/**",
    "**/__tests__/**",
    "**/*.d.ts",
    "**/__mocks__/**",
    "**/*.config.js",
    "**/*.setup.ts",
    "**/*.test.ts",
    "**/*.test.tsx"
  ],
  "ignoreUnimported": [
    "**/queries.ts",
    "**/mocks.ts",
    "**/mocks.tsx",
    "**/testData.ts",
    "**/testSetup.tsx",
    "**/MockedProvider.tsx",
    "**/hooks/useMockRouter.ts"
  ],
  "ignoreUnused": ["next", "react", "react-dom"],
  "ignoreUnresolved": []
}
