{
  "name": "@dolt-sql-workbench/graphql-server",
  "version": "0.1.0",
  "description": "",
  "author": "DoltHub",
  "private": true,
  "packageManager": "yarn@4.5.0",
  "installConfig": {
    "hoistingLimits": "dependencies"
  },
  "scripts": {
    "ci": "yarn prettier && yarn compile && yarn lint && yarn test && yarn build && yarn check-server",
    "build": "nest build",
    "check-server": "./ci-helper.sh ./",
    "compile": "tsc -b",
    "dev": "NODE_ENV=development nest start --watch",
    "dev:logs-off": "NODE_ENV=development LOGS_OFF=true nest start --watch",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "prettier": "prettier --check 'src/**/*.{js,ts}'",
    "prettier-fix": "prettier --write 'src/**/*.{js,ts}'",
    "lint": "eslint --cache --ext .ts,.js,.tsx,.jsx .",
    "lint-errors": "eslint --quiet --cache --ext .ts,.js,.tsx,.jsx .",
    "test": "jest --silent",
    "test:loud": "jest",
    "yalc:react-lib": "yalc link @dolthub/react-hooks && yalc link @dolthub/web-utils",
    "yalc:clean": "yalc remove --all"
  },
  "engines": {
    "node": ">=10.3.0"
  },
  "dependencies": {
    "@apollo/server": "^4.11.0",
    "@dolthub/web-utils": "^0.1.2",
    "@nestjs/apollo": "^12.2.0",
    "@nestjs/cli": "^10.4.5",
    "@nestjs/common": "^10.4.4",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.4.4",
    "@nestjs/graphql": "^12.2.0",
    "@nestjs/platform-express": "^10.4.4",
    "@nestjs/schematics": "^10.1.4",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/typeorm": "^10.0.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "graphql": "^16.9.0",
    "graphql-upload": "15.0.0",
    "mysql2": "^3.11.0",
    "pg": "^8.12.0",
    "pg-copy-streams": "^6.0.6",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.5.4",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1",
    "@types/cors": "^2",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.5.2",
    "@types/semver": "^7",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.8.0",
    "eslint": "^8.52.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-jest": "^28.8.3",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.2.4",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.5.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.2",
    "webpack": "^5.88.1",
    "yalc": "^1.0.0-pre.53"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".test.ts$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  },
  "resolutions": {
    "express": "4.21.0"
  }
}
