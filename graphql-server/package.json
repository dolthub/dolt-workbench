{
  "name": "@dolt-sql-workbench/graphql-server",
  "version": "0.1.0",
  "description": "",
  "author": "DoltHub",
  "private": true,
  "scripts": {
    "ci": "yarn prettier && yarn compile && yarn lint && yarn test && yarn build && yarn check",
    "build": "nest build",
    "check-server": "./ci-helper.sh ./",
    "compile": "tsc -b",
    "dev": "NODE_ENV=development nest start --watch",
    "dev:logs-off": "NODE_ENV=development LOGS_OFF=true nest start --watch",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "prettier": "prettier --check 'src/**/*.{js,ts}'",
    "prettier-fix": "prettier --write 'src/**/*.{js,ts}'",
    "lint": "eslint --cache --ext .ts,.js,.tsx,.jsx .",
    "lint-errors": "eslint --quiet --cache --ext .ts,.js,.tsx,.jsx .",
    "test": "jest --passWithNoTests"
  },
  "engines": {
    "node": ">=10.3.0"
  },
  "dependencies": {
    "@apollo/server": "^4.7.3",
    "@nestjs/apollo": "^12.0.7",
    "@nestjs/cli": "^10.1.7",
    "@nestjs/common": "^10.2.7",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.5",
    "@nestjs/graphql": "^12.0.7",
    "@nestjs/platform-express": "^10.2.7",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/terminus": "^10.0.1",
    "@nestjs/typeorm": "^10.0.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "graphql": "^16.7.1",
    "graphql-upload": "13",
    "mysql2": "^3.1.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.4",
    "timeago.js": "^4.0.2",
    "typeorm": "^0.3.12"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1",
    "@types/cors": "^2",
    "@types/node": "^20.8.4",
    "@types/semver": "^7",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.52.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.6.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.5.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2",
    "webpack": "^5.88.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".test.ts$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
