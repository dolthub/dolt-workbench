{
  "name": "@dolt-sql-workbench/graphql-server",
  "version": "0.1.0",
  "description": "",
  "author": "DoltHub",
  "private": true,
  "packageManager": "yarn@4.11.0",
  "scripts": {
    "ci": "yarn prettier && yarn compile && yarn lint && yarn test && yarn build && yarn check-server",
    "build": "nest build",
    "check-server": "./ci-helper.sh ./",
    "compile": "tsc -b",
    "dev": "NODE_ENV=development nest start --watch",
    "dev:logs-off": "NODE_ENV=development LOGS_OFF=true nest start --watch",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "prettier": "prettier --check 'src/**/*.{js,ts}'",
    "prettier-fix": "prettier --write 'src/**/*.{js,ts}'",
    "lint": "eslint --cache --ext .ts,.js,.tsx,.jsx .",
    "lint-errors": "eslint --quiet --cache --ext .ts,.js,.tsx,.jsx .",
    "test": "jest --silent",
    "test:loud": "jest",
    "yalc:react-lib": "yalc link @dolthub/react-hooks && yalc link @dolthub/web-utils",
    "yalc:clean": "yalc remove --all"
  },
  "engines": {
    "node": ">=10.3.0"
  },
  "dependencies": {
    "@apollo/server": "^4.12.0",
    "@dolthub/web-utils": "^0.1.9",
    "@nestjs/apollo": "^13.1.0",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.12",
    "@nestjs/graphql": "^13.0.4",
    "@nestjs/platform-express": "^11.1.8",
    "@nestjs/schematics": "^11.0.7",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/typeorm": "^11.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "graphql": "^16.11.0",
    "graphql-upload": "15.0.0",
    "mysql2": "^3.11.0",
    "pg": "^8.16.3",
    "pg-copy-streams": "^7.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "typeorm": "^0.3.26"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1",
    "@types/cors": "^2",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@types/semver": "^7",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.35.1",
    "eslint": "^9.34.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "jest": "^30.2.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.0",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.5.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "webpack": "^5.102.0",
    "yalc": "^1.0.0-pre.53"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".test.ts$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  },
  "resolutions": {
    "brace-expansion": "2.0.2",
    "multer": "2.0.2",
    "sha.js": "2.4.12",
    "tmp": "0.2.4"
  }
}
