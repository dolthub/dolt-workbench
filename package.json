{
  "private": true,
  "name": "dolt-workbench",
  "productName": "dolt-workbench",
  "version": "1.0.0",
  "main": "app/background.js",
  "scripts": {
    "dev": "cd graphql-server && nest build && cd .. && nextron",
    "build": "cd graphql-server && nest build && cd .. && nextron build",
    "build:mac": "nextron build --mac",
    "postinstall": "electron-builder install-app-deps",
    "clean": "tsc -b --clean && yarn yalc:clean",
    "compile": "tsc -b",
    "dev:web": "next dev -p 3002 web",
    "build:web": "next build web",
    "start:web": "next start web",
    "compile:web": "tsc -b web",
    "dev:graphql": "cd graphql-server && NODE_ENV=development nest start --watch",
    "compile:graphql": "tsc -b graphql-server",
    "build:graphql": "cd graphql-server && nest build",
    "prettier-fix": "prettier --write 'web/{components,contexts,hooks,lib,pages}/**/*.{js,ts,jsx,tsx,css}'"
  },
  "build": {
    "appId": "com.dolthub.dolt-workbench",
    "files": [
      "app",
      "package.json"
    ],
    "extraFiles": [
      "graphql-server/dist/*",
      "graphql-server/dist/**/*"
    ],
    "mac": {
      "icon": "build/icon.icns",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "artifactName": "${productName}-mac-${arch}.${ext}",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ]
    },
    "includeSubNodeModules": false,
    "compression": "maximum",
    "asar": true
  },
  "dependencies": {
    "@apollo/client": "^3.9.9",
    "@apollo/server": "^4.10.2",
    "@dolthub/react-components": "^0.2.6",
    "@dolthub/react-contexts": "^0.1.0",
    "@dolthub/react-hooks": "^0.1.7",
    "@dolthub/web-utils": "^0.1.8",
    "@nestjs/apollo": "^12.1.0",
    "@nestjs/cli": "^10.4.2",
    "@nestjs/common": "^10.3.10",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.3.8",
    "@nestjs/graphql": "^12.1.1",
    "@nestjs/platform-express": "^10.3.10",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/typeorm": "^10.0.2",
    "@react-icons/all-files": "^4.1.0",
    "ace-builds": "^1.34.2",
    "apollo-upload-client": "^17.0.0",
    "chance": "^1.1.11",
    "classnames": "^2.3.2",
    "commit-graph": "^2.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "diff": "^5.1.0",
    "github-markdown-css": "^5.3.0",
    "graphql": "^16.8.1",
    "graphql-upload": "15.0.0",
    "isomorphic-unfetch": "^4.0.2",
    "localforage": "^1.10.0",
    "localforage-getitems": "^1.4.2",
    "lodash": "^4.17.21",
    "mysql2": "^3.11.0",
    "next": "^14.2.4",
    "next-useragent": "^2.8.0",
    "node-sql-parser": "^4.17.0",
    "pg": "^8.12.0",
    "pg-copy-streams": "^6.0.6",
    "react": "^18.3.1",
    "react-ace": "^12.0.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-data-grid": "7.0.0-beta.44",
    "react-diff-viewer": "^3.1.1",
    "react-dom": "^18.2.0",
    "react-infinite-scroller": "^1.2.6",
    "react-timeago": "^7.2.0",
    "reactflow": "^11.11.4",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.5.4",
    "sharp": "^0.33.4",
    "swr": "^2.2.4",
    "tailwindcss": "^3.3.5",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/fragment-matcher": "^5.0.0",
    "@graphql-codegen/typescript-operations": "^4.2.3",
    "@graphql-codegen/typescript-react-apollo": "^4.0.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/apollo-upload-client": "^17",
    "@types/chance": "^1.1.5",
    "@types/cookie-parser": "^1",
    "@types/cors": "^2",
    "@types/diff": "^5.2.1",
    "@types/jest": "^29.5.6",
    "@types/lodash": "^4.14.202",
    "@types/node": "^22.5.2",
    "@types/react": "^18.3.5",
    "@types/react-copy-to-clipboard": "^5.0.6",
    "@types/react-dom": "^18.2.18",
    "@types/react-infinite-scroller": "^1.2.3",
    "@types/react-timeago": "^4.1.4",
    "@types/semver": "^7",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.14.1",
    "autoprefixer": "^10.4.19",
    "babel-jest": "^29.7.0",
    "cssnano": "^6.0.3",
    "electron": "^31.0.1",
    "electron-builder": "^24.13.3",
    "electron-serve": "^1.3.0",
    "electron-store": "^8.2.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
